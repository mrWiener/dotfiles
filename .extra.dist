# PATH additions
for another_bin in \
    $HOME/bin \
    /usr/local/php5/bin \
    $HOME/.rbenv/bin
do
    [[ -e $another_bin ]] && export PATH=$another_bin:$PATH
done
export PATH=/usr/local/bin:/usr/local/sbin:$PATH

hash rbenv &>/dev/null
if [ $? -eq 0 ]; then
	eval "$(rbenv init -)"
	rbenv global 1.9.3-p194
fi